  <Code>
    Public Function SumArray(ByVal apValues As Object()) As Double
    Dim fTotal As Double
    Dim pCurValue As Object

    For Each pCurValue In apValues
    fTotal += Convert.ToDouble(pCurValue)
    Next

    Return fTotal
    End Function
  </Code>
